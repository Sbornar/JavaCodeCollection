import java.util.*;

public class Main {
	static void printword(int N)
    {
        //Write your code here
        if(N==1)
        {
            System.out.println("AccioSchool");
            return;
        }
        printword(N-1);
        System.out.println("AccioSchool");

    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n= sc.nextInt();
	    printword(n);
    }
}
