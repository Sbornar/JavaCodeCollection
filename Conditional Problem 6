import java.util.*;

public class Main {
    public static void main(String[] args) throws Throwable {
        

        int a ,b;

        Scanner src = new Scanner(System.in);

        a = src.nextInt();
        b = src.nextInt();

        if(a % 2 != 0 && b % 2 !=0)
        {
            System.out.println("we are odd");
        }else
        {
            System.out.println("we are simple");
        }

    }
}
