import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
           // write your code here.

           int Ivalue;
           int Icount = 1;

           Scanner scn = new Scanner(System.in);

           Ivalue = scn.nextInt();

           while(Icount <= 20)
           {
             System.out.println(Ivalue+" x "+Icount+" = "+Ivalue*Icount);
             Icount++;
           }

    }
}
