import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        
        int a , b , c;

        Scanner scn = new Scanner(System.in);

        a = scn.nextInt();
        b = scn.nextInt();
        c = scn.nextInt();

        if(a >= b && a >= c)
        {
            System.out.println(a);
        }else if(b >= a && b >= c)
        {
            System.out.println(b);
        
        }else 
        {
            System.out.println(c);
        }


    }
}
