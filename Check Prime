import java.util.*;


public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n= sc.nextInt();
        // your code here
		sc.close();
        
        boolean bvalue = true;
        int Count = 2;

        while(Count < n)
        {
            if(n % Count ==0)
            {
                bvalue = false;
            }
            Count++;
        }

        if(bvalue == true)
        {
            System.out.println(n+" is a prime number");
        }else
        {
            System.out.println(n+" is not a prime number");
        }



    }
}
