import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
         // write your code here
         int Ivalue;
         int Icount = 1;

         Scanner scn = new Scanner(System.in);
         
         Ivalue = scn.nextInt();

         while(Ivalue >= Icount)
         {
            System.out.print(Ivalue+" ");
            Ivalue--;
         }

    }
}
