import java.util.*;

public class Main {

	static String ModifyName(String name) {
        // write code here

      String value = name+" is a Super Hero";

        return value;
    }
	
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        

        String name = sc.nextLine();
        System.out.println(ModifyName(name));   
    }
}
